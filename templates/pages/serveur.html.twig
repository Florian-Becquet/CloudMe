    <tr>
      <th scope="row">{{ info.id }}</th>
      <td>{{ info.architecture }}</td>
      <td>{{ info.description }}</td>
      <td>{{ info.headline }}</td>
    </tr>
  </tbody>
  {% endfor %}
</table>
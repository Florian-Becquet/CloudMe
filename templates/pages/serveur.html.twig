
<table class="table">
  <thead >
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Achitecture</th>
      <th scope="col">Description</th>
      <th scope="col">Headline</th>
    </tr>
  </thead>
  {% for info in vps %}
